# 
# KADU MODULES CONFIGURATION FILE
#
# y - yes, compiled into kadu (static)
# m - module, compiles as .so (shared)
# n - no, module disabled
#

# Tray icon support
module_docking=y

# XLib tray docking techniques module
module_x11_docking=y

# WindowMaker docking module
module_wmaker_docking=n

# OpenSSL encrypted chat support
module_encryption=y

# Speech synthesis support
module_speech=n

# Voice chat support
module_voice=y

# General sound support
module_sound=y

# External application sound support
module_ext_sound=y

# Direct /dev/dsp sound support
module_dsp_sound=m

# Arts sound server support
module_arts_sound=n

# ESD sound server support
module_esd_sound=n

# Network Audio System support
module_nas_sound=n

# SMS module
module_sms=y

# default gateways for SMS module
module_default_sms=y

# dcc module with file transfer support
module_dcc=y

# Auto away module
module_autoaway=y

# Autoresponder module
module_autoresponder=m

# Echo sample module
module_echo=n

#
# Below are some external modules, made by third parties, but
# accepted by Kadu Team and quite well supported on our forum. When
# you choose to install any of these, the sources will be downloaded
# from their original web pages, so don't blame us when they are
# (maybe just temporarily) unavailable.
#

# TCL scripting support and KaduPro extensions
module_tcl_scripting=n

# Xmms player support
module_xmms=n

# AmaroK player support
module_amarok=n

# Description from file module
module_filedesc=n

# Mail checker module
module_mail=n

# Spellchecker module (Aspell support)
module_spellchecker=n

# PC-Speaker support
module_pcspeaker=n
