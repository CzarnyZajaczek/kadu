# 
# KADU MODULES CONFIGURATION FILE
#
# y - yes, compiled into kadu (static)
# m - module, compiles as .so (shared)
# n - no, module disabled
#

# Tray icon support
module_docking=y

# XLib tray docking techniques module
module_x11_docking=y

# OpenSSL encrypted chat support
module_encryption=y

# Speech synthesis support
module_speech=n

# Voice chat support
module_voice=y

# General sound support
module_sound=y

# External application sound support
module_ext_sound=y

# Direct /dev/dsp sound support
module_dsp_sound=m

# Arts sound server support
module_arts_sound=n

# ESD sound server support
module_esd_sound=n

# Network Audio System support
module_nas_sound=n

# SMS module
module_sms=y

# default gateways for SMS module
module_default_sms=y

# Auto away module
module_autoaway=y

# Autoresponder module
module_autoresponder=m

# Echo sample module
module_echo=n
