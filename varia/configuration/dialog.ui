	<section name="Chat">
		<tab name="Behaviour">
			<group-box name="Behaviour">
				<check-box caption="Open chat window on new message" config-section="Chat" config-item="OpenChatOnMessage" />
				<check-box caption="Wait for message delivery" config-section="Chat" config-item="MessageAcks" />
				<check-box caption="'Enter' in chat send message" config-section="Chat" config-item="AutoSend" />
				<check-box caption="Confirm clearing chat window" config-section="Chat" config-item="ConfirmChatClear" />
			</group-box>

			<group-box name="Closing">
				<check-box caption="Block window close on new message" config-section="Chat" config-item="ChatCloseTimer" id="chatCloseTimer" />
				<spin-box caption="Max time to block window close" config-section="Chat" config-item="ChatCloseTimerPeriod" min-value="1" max-value="30" step="1" id="chatCloseTimerPeriod" />
			</group-box>
		</tab>

		<tab name="Images">
			<group-box name="Images">
				<check-box caption="Receive images" config-section="Chat" config-item="ReceiveImages" id="receiveImages" />
				<check-box caption="Receive images during invisibility" config-section="Chat" config-item="RecevieImagesDuringInvisibility" id="receiveImagesDuringInvisibility" />
			</group-box>
		</tab>

		<tab name="WWW">
			<group-box name="WWW browser">
				<combo-box caption="Choose your browser" config-section="General" config-item="ChooseYourBrowser" />
				<combo-box caption="Browser options" config-section="General" config-item="BrowserOptions" />
				<line-edit caption="Custom browser" config-section="General" config-item="CustomBrowser" />
			</group-box>
		</tab>
	</section>

	<section name="Advanced">

		<tab name="Kadu">
			<group-box name="Parser">
				<check-box caption="Allow executing commands by parser" config-section="General" config-item="AllowExecutingFromParser" />
			</group-box>
			<group-box name="Debugging">
				<line-edit caption="Debugging mask" config-section="General" config-item="DEBUG_MASK" />
			</group-box>
		</tab>

		<tab name="Status">
				<check-box caption="User KaduParser to parse status" config-section="General" config-item="ParseStatus" />
		</tab>

		<tab name="Chat">
			<group-box name="Headers and separators">
				<check-box caption="Remove chat header repetitions" config-section="General" config-item="RemoveChatHeaderRepetitions" />
				<spin-box caption="Chat header separators height" config-section="Look" config-item="ChatHeaderSeparatorsHeight" min-value="1" max-value="1000" step="1" />
				<spin-box caption="Interval between header removal" config-section="Look" config-item="IntervalBetweenHeaderRemoval" min-value="1" max-value="1000" step="1" />
			</group-box>

			<group-box name="Server time">
				<check-box caption="Remove server time" config-section="General" config-item="RemoveServerTime" id="removeServerTime" />
				<spin-box caption="Maximum time difference" config-section="Look" config-item="MaxTimeDifference" min-value="1" max-value="1000" step="1" id="maxTimeDifference" />
			</group-box>

			<group-box name="Advanced">
				<line-edit caption="Full chat style" config-section="General" config-item="FullChatStyle" />
				<line-edit caption="Chat background image (experimental) (button)" config-section="General" config-item="ChatBgImg" />
			</group-box>

			<group-box name="Window title">
				<line-edit caption="Chat window title syntax" config-section="General" config-item="ChatWindowTitleSyntax" />
				<line-edit caption="Conference window title prefix" config-section="General" config-item="ConferencePrefix" />
				<line-edit caption="Conference window title syntax" config-section="General" config-item="ConferenceSyntax" />
			</group-box>
		</tab>

		<tab name="Userbox">

			<group-box name="Userbox background">
				<line-edit caption="Userbox background image (button)" config-section="General" config-item="UserboxBgImg" />
				<check-box caption="Change image size" config-section="General" config-item="ChangeImgSize" />
				<spin-box caption="Start at [X]" config-section="Look" config-item="StartAtX" min-value="1" max-value="1000" step="1" />
				<spin-box caption="Start at [Y]" config-section="Look" config-item="StartAtY" min-value="1" max-value="1000" step="1" />
				<spin-box caption="Image width" config-section="Look" config-item="ImgWidth" min-value="1" max-value="1000" step="1" />
				<spin-box caption="Image height" config-section="Look" config-item="ImgHeight" min-value="1" max-value="1000" step="1" />
			</group-box>
		</tab>

		<tab name="Information panel">
			<group-box name="Syntax">
				<line-edit caption="Information panel syntax" config-section="General" config-item="InformationPanelSyntax" />
			</group-box>
		</tab>

		<tab name="Images">
			<group-box name="Images">
<!-- 				<spin-box caption="Max image size" config-section="General" config-item="MaxImageSize" min-value="1" max-value="255" step="1" /> -->
				<spin-box caption="Limit numbers of image recevied per minute" config-section="General" config-item="ImagesLimit" min-value="1" max-value="30" step="1" />
			</group-box>
		</tab>
	</section>

</configuration-ui>
